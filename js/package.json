    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^30.2.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
